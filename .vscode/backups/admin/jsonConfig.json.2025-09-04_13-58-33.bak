{
    "i18n": true,
    "type": "tabs",
    "items": {
        "_main": {
            "type": "panel",
            "label": "Main settings",
            "items": {
                "FeedInDataPoint": {
                    "type": "objectId",
                    "label": "feedInDataPoint",
                    "default": "",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
                    "xl": 6
                },
                "baseload": {
                    "type": "number",
                    "label": "baseload",
                    "default": 100,
                    "min": 0,
                    "max": 10000,
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
                    "xl": 6
                },
                "feedinNegativ": {
                    "type": "checkbox",
                    "label": "feedinNegativ",
                    "default": false,
                    "xs": 12,
                    "sm": 12,
                    "md": 12,
                    "lg": 12,
                    "xl": 12
                },
                "notifyBinary": {
                    "type": "checkbox",
                    "label": "notifyBinary",
                    "default": false,
                    "xs": 12,
                    "sm": 12,
                    "md": 3,
                    "lg": 3,
                    "xl": 3
                },
                "notifyHeating": {
                    "type": "checkbox",
                    "label": "notifyHeating",
                    "default": false,
                    "xs": 12,
                    "sm": 12,
                    "md": 3,
                    "lg": 3,
                    "xl": 3
                },
                "notifyPercent": {
                    "type": "checkbox",
                    "label": "notifyPercent",
                    "default": false,
                    "xs": 12,
                    "sm": 12,
                    "md": 3,
                    "lg": 3,
                    "xl": 3
                },
                "notifyBattery": {
                    "type": "checkbox",
                    "label": "notifyBattery",
                    "default": false,
                    "xs": 12,
                    "sm": 12,
                    "md": 3,
                    "lg": 3,
                    "xl": 3
                }
            }
        },
        "_consumer": {
            "type": "panel",
            "label": "consumer-Panel",
            "items": {
                "consumer": {
                    "type": "table",
                    "label": "consumerList",
                    "xs": 12,
                    "sm": 12,
                    "md": 12,
                    "lg": 8,
                    "xl": 8,
                    "showSecondAddAt": 10,
                    "useCardFor": ["xs", "sm", "md", "lg", "xl"],
                    "compact": true,
                    "items": [
                        {
                            "type": "text",
                            "attr": "name",
                            "label": "name"
                        },
                        {
                            "type": "checkbox",
                            "attr": "enabled",
                            "label": "active",
                            "default": false
                        },
                        {
                            "type": "objectId",
                            "attr": "datapoint",
                            "label": "booleanControlDataPoint",
                            "tooltip": "tooltip_booleanControlDataPoint"
                        },
                        {
                            "type": "objectId",
                            "attr": "numericDatapoint",
                            "label": "numericDatapoint",
                            "tooltip": "tooltip_numericDatapoint"
                        },
                        {
                            "type": "staticText",
                            "label": "batteryManagement"
                        },
                        {
                            "type": "objectId",
                            "attr": "batterySetpoint",
                            "label": "batterySetpoint",
                            "tooltip": "tooltip_batterySetpoint"
                        },
                        {
                            "type": "objectId",
                            "attr": "batterySOC",
                            "label": "batterySOC",
                            "tooltip": "tooltip_batterySOC"
                        },
                        {
                            "type": "objectId",
                            "attr": "batteryTargetSOC",
                            "label": "batteryTargetSOC",
                            "tooltip": "tooltip_batteryTargetSOC"
                        },
                        {
                            "type": "objectId",
                            "attr": "batteryControlModeDatapoint",
                            "label": "batteryControlModeDatapoint",
                            "tooltip": "tooltip_batteryControlModeDatapoint"
                        },
                        {
                            "type": "staticText",
                            "label": "heatingElement",
                            "newLine": true
                        },
                        {
                            "type": "objectId",
                            "attr": "temperatureDatapoint",
                            "label": "temperatureDatapoint",
                            "tooltip": "tooltip_temperatureDatapoint"
                        },
                        {
                            "type": "number",
                            "attr": "maxTemperature",
                            "label": "maxTemperature",
                            "tooltip": "tooltip_maxTemperature",
                            "min": 40,
                            "max": 80,
                            "default": 40
                        },
                        {
                            "type": "number",
                            "attr": "minTemperature",
                            "label": "minTemperature",
                            "tooltip": "tooltip_minTemperature",
                            "min": 25,
                            "max": 50,
                            "default": 25
                        },
                        {
                            "type": "objectId",
                            "attr": "enableDatapoint",
                            "label": "enableDatapoint",
                            "tooltip": "tooltip_enableDatapoint"
                        },
                        {
                            "type": "objectId",
                            "attr": "heartbeatDatapoint",
                            "label": "heartbeatDatapoint",
                            "tooltip": "tooltip_heartbeatDatapoint",
                            "default": ""
                        },
                        {
                            "type": "staticText",
                            "label": "consumerSettings",
                            "newLine": true
                        },
                        {
                            "type": "number",
                            "attr": "performance",
                            "label": "performance",
                            "tooltip": "tooltip_performance",
                            "min": 1
                        },
                        {
                            "type": "number",
                            "attr": "switchOnPoint",
                            "label": "switchOnPoint",
                            "tooltip": "tooltip_switchOnPoint",
                            "min": 1
                        },
                        {
                            "type": "number",
                            "attr": "switchOffPoint",
                            "label": "switchOffPoint",
                            "tooltip": "tooltip_switchOffPoint",
                            "min": 0
                        },
                        {
                            "type": "select",
                            "attr": "ruletype",
                            "label": "ruletype",
                            "options": [
                                {
                                    "value": "binary",
                                    "label": "binary"
                                },
                                {
                                    "value": "percent",
                                    "label": "percent"
                                },
                                {
                                    "value": "battery",
                                    "label": "battery"
                                },
                                {
                                    "value": "heating",
                                    "label": "heating"
                                }
                            ],
                            "default": "binary"
                        },
                        {
                            "type": "number",
                            "attr": "minPercentStart",
                            "label": "minPercentStart",
                            "tooltip": "tooltip_minPercentStart",
                            "min": 0,
                            "max": 100,
                            "default": 10
                        },
                        {
                            "type": "number",
                            "attr": "minSOC",
                            "label": "minSOC",
                            "tooltip": "tooltip_minSOC",
                            "min": 0,
                            "max": 100,
                            "default": 20
                        },
                        {
                            "type": "text",
                            "attr": "switchOnTime",
                            "label": "switchOnTime",
                            "default": ""
                        },
                        {
                            "type": "text",
                            "attr": "switchOffTime",
                            "label": "switchOffTime",
                            "default": ""
                        },
                        {
                            "type": "number",
                            "attr": "switchOnDelay",
                            "label": "switchOnDelay",
                            "tooltip": "tooltip_switchOnDelay",
                            "min": 0,
                            "default": 20
                        },
                        {
                            "type": "number",
                            "attr": "switchOffDelay",
                            "label": "switchOffDelay",
                            "tooltip": "tooltip_switchOffDelay",
                            "min": 0,
                            "default": 20
                        },
                        {
                            "type": "checkbox",
                            "attr": "alwaysOffAtTime",
                            "label": "alwaysOffAtTime",
                            "default": false
                        },
                        {
                            "type": "number",
                            "attr": "priority",
                            "label": "priority",
                            "tooltip": "tooltip_priority",
                            "min": 1,
                            "default": 1
                        }
                    ]
                }
            }
        },
        "_notifications": {
            "type": "panel",
            "label": "notifications",
            "items": {
                "telegramInstance": {
                    "type": "instance",
                    "label": "telegramInstance",
                    "adapter": "telegram",
                    "default": "",
                    "xs": 12,
                    "sm": 12,
                    "md": 12,
                    "lg": 6,
                    "xl": 6
                },
                "telegramRecipients": {
                    "type": "text",
                    "label": "telegramRecipients",
                    "default": "",
                    "xs": 12,
                    "sm": 12,
                    "md": 12,
                    "lg": 6,
                    "xl": 6
                },
                "gotifyInstance": {
                    "type": "instance",
                    "label": "gotifyInstance",
                    "adapter": "gotify",
                    "default": "",
                    "xs": 12,
                    "sm": 12,
                    "md": 12,
                    "lg": 12,
                    "xl": 12
                },
                "emailInstance": {
                    "type": "instance",
                    "label": "emailInstance",
                    "adapter": "email",
                    "default": ""
                }
            }
        }
    }
}
